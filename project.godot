; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between [ ]
;   param=value ; assign values to parameters
;
config_version=5

[application]
config/name="Idle Tap Cash: Evergreen"
runtime/disable_stdout=true
run/main_scene="res://scenes/Main.tscn"
config/icon="res://assets/ui/icon.png"
boot_splash/image="res://assets/ui/icon.png"

[rendering]
quality/driver/driver_name="GLES3"

[autoload]
ByteBrewEntry="*res://addons/bytebrew/ByteBrewEntry.gd"
Analytics="*res://scripts/Analytics.gd"
AdManager="*res://scripts/AdManager.gd"
IAPManager="*res://scripts/IAPManager.gd"
RemoteConfig="*res://scripts/RemoteConfig.gd"
GameState="*res://scripts/GameState.gd"
Social="*res://scripts/Social.gd"
LiveOps="*res://scripts/LiveOps.gd"
PiggyBank="*res://scripts/PiggyBank.gd"
LevelManager="*res://scripts/LevelManager.gd"
Offers="*res://scripts/Offers.gd"
SeasonPass="*res://scripts/SeasonPass.gd"
Experiments="*res://scripts/Experiments.gd"
Localize="*res://scripts/Localize.gd"
Quests="*res://scripts/Quests.gd"
Notifications="*res://scripts/Notifications.gd"
Economy="*res://scripts/Economy.gd"
Geo="*res://scripts/Geo.gd"
ConsentManager="*res://scripts/ConsentManager.gd"
IOSPriceBridge="*res://scripts/IOSPriceBridge.gd"
Teams="*res://scripts/Teams.gd"
Tournament="*res://scripts/Tournament.gd"
Bingo="*res://scripts/Bingo.gd"
Haptics="*res://scripts/Haptics.gd"
Missions="*res://scripts/Missions.gd"
EconomyTelemetry="*res://scripts/EconomyTelemetry.gd"
Treasure="*res://scripts/Treasure.gd"
CMP="*res://scripts/CMP.gd"
AdMediation="*res://scripts/AdMediation.gd"
ATT="*res://scripts/ATT.gd"

[android]
modules/use_custom_build=true
plugins/Enabled=PackedStringArray("ByteBrew", "AdsBridge", "IAPBridge")

[ios]
modules/use_custom_build=true
plugins/Enabled=PackedStringArray("ByteBrew")

[display]
window/size/width=720
window/size/height=1280
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
